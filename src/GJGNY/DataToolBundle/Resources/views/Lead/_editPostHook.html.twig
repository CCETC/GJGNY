<script>
    
    $('select[id$="_Program"]').live("change", function(e){

        $.ajax({
            url: '{{ url('getProgramDate') }}/' + $(this).val(),
            success: function(html) {
                if(html != "") {
                    $('input[id$="_DateOfLead"]').val(html);
                }
            }
        });
    });

</script>