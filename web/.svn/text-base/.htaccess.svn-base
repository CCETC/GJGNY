RewriteEngine On

RewriteCond %{HTTP_HOST}    ^upgradeupstate\.org [nocase]
RewriteRule (.*)    http://www.upgradeupstate.org%{REQUEST_URI} [redirect,last]

RewriteCond %{HTTP_HOST}    ^upgradeupstate\.com [nocase]
RewriteRule (.*)    http://www.upgradeupstate.org%{REQUEST_URI} [redirect,last]

RewriteCond %{HTTP_HOST}    ^www.upgradeupstate\.com [nocase]
RewriteRule (.*)    http://www.upgradeupstate.org%{REQUEST_URI} [redirect,last]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)$ app.php [QSA,L]
